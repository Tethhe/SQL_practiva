CREATE TABLE CLIENTE (
    ID_CLIENTE          integer     generated by default as identity increment by 1 start with 1 cache 20 order nocycle not null enable primary key, 
    RUC                 VARCHAR(11),
    NOMBRE              VARCHAR(120),
    DIRECCION           VARCHAR(200),
);

CREATE TABLE PRODUCTO (
    ID_PRODUCTO         integer     generated by default as identity increment by 1 start with 1 cache 20 order nocycle not null enable primary key, 
    NOMBRE              VARCHAR(120),
    PRECIO              FLOAT,
    STOCK               DECIMAL
) 
