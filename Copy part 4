CREATE TABLE DETALLE_VENTA(
    CANTIDAD            DECIMAL,
    PRECIO              FLOAT,
    SUB_TOTAL           FLOAT,
    ID_VENTA            INTEGER,
    ID_PRODUCTO         INTEGER,
    CONSTRAINT DV_VENTAO_FK FOREIGN KEY (ID_VENTA) REFERENCES VENTA (ID_VENTA),
    CONSTRAINT DV_PRODUCTO_FK FOREIGN KEY (ID_PRODUCTO) REFERENCES PRODUCTO (ID_PRODUCTO)        
)
