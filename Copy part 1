create table CATEGORIA (
    ID_CATEGORIA        integer     generated by default as identity increment by 1 start with 1 cache 20 order nocycle not null enable primary key, 
    NOMBRE              VARCHAR(120),
    DESCRIPCION         VARCHAR(240)
);

CREATE TABLE TELEFONO (
    ID_TELEFONO        integer     generated by default as identity increment by 1 start with 1 cache 20 order nocycle not null enable primary key, 
    NUMERO             VARCHAR(15)
    );

CREATE TABLE PROVEEDOR (
    ID_PROVEEDOR       integer     generated by default as identity increment by 1 start with 1 cache 20 order nocycle not null enable primary key, 
    RAZON_SOCIAL        VARCHAR(60),
    RUC                 VARCHAR(11),
    DIRECCION           VARCHAR(200),
    TELEFONO            VARCHAR(15),
    WEB                 VARCHAR(100)
);

--copiar uno por uno
